def music_chart():
    dict_ = {}
    while input != 'off':
        place = input('введите место : ')
        if place != 'off':
            singer = input('введите исполнителя: ')
            if singer != 'off':
                name = input('введите трек: ')
                if name != 'off':
                    if place and singer and name:
                        dict_[place, singer] = name
                else:
                    print(dict_)
            else:
                print(dict_)
        else:
            print(dict_)
    print(dict_)


music_chart()
